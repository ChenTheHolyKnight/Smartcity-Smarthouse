<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="il.ac.technion.cs.eldery.system.gui.applications.ComboWindowController">
	<children>
		<VBox spacing="10.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0" fx:id="topBox">
			<children>
				<ComboBox fx:id="comboBox" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
				<HBox fx:id="ButtonBox">
					<children>
						<Button fx:id="install" mnemonicParsing="false" prefHeight="25.0"
							prefWidth="140.0" text="install" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0" />
						<Button fx:id="cancel" mnemonicParsing="false" prefHeight="25.0"
							prefWidth="140.0" text="Cacncel" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>

